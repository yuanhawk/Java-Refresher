package business;

import java.util.ArrayList;

public class LineInv implements LineInvReader {
	
	private ArrayList<InvCalc> ic;
	
	public LineInv() {
		ic = new ArrayList<InvCalc>();
	}

	public void addLineInv(InvCalc invCalc) {		
		ic.add(invCalc);
	}
	
	public ArrayList<InvCalc> getLineInv() {
		return ic;
	}

}
