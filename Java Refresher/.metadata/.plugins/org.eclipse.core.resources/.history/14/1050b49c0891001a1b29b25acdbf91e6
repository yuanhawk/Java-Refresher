package business;

public class Employee {
	
	private String name, desc;
	
	public Employee() {
		name = "";
		desc = "";
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getDesc() {
		return desc;
	}

	public void setDesc(String desc) {
		this.desc = desc;
	}
	
	

}
