package business;

import java.util.ArrayList;

public class LineInv implements LineInvReader {
	
	private ArrayList<InvCalc> ic = null;
	
	public LineInv() {
		ic = new ArrayList<InvCalc>();
	}

	public ArrayList<InvCalc> getIc() {
		return ic;
	}

	public void setIc(ArrayList<InvCalc> ic) {
		this.ic = ic;
	}

	public void addLineInv(InvCalc inc) {		
		ic.add(inc);
	}
	
	public ArrayList<InvCalc> getLineInv() {
		return ic;
	}

}
